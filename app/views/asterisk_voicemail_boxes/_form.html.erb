<%= simple_form_for(@asterisk_voicemail_box) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :prefix %>
    <%= f.input :extension %>
    <%= f.input :did %>
    <%= f.input :display_name %>
    <%= f.association :asterisk_user %>
    <%= f.input :password %>
    <%= f.input :email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
