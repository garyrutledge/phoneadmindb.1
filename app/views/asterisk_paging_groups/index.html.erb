<h1>Listing asterisk_paging_groups</h1>

<table>
  <thead>
    <tr>
      <th>Paging extension</th>
      <th>Name</th>
      <th>Members</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asterisk_paging_groups.each do |asterisk_paging_group| %>
      <tr>
        <td><%= asterisk_paging_group.paging_extension %></td>
        <td><%= asterisk_paging_group.name %></td>
        <td><%= asterisk_paging_group.members %></td>
        <td><%= link_to 'Show', asterisk_paging_group %></td>
        <td><%= link_to 'Edit', edit_asterisk_paging_group_path(asterisk_paging_group) %></td>
        <td><%= link_to 'Destroy', asterisk_paging_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asterisk paging group', new_asterisk_paging_group_path %>
