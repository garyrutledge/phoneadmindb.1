<h1>Listing asterisk_devices</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Location</th>
      <th>Secret</th>
      <th>Asterisk user</th>
      <th>Admin password</th>
      <th>Mac address</th>
      <th>Account code</th>
      <th>Associated user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asterisk_devices.each do |asterisk_device| %>
      <tr>
        <td><%= asterisk_device.description %></td>
        <td><%= asterisk_device.location %></td>
        <td><%= asterisk_device.secret %></td>
        <td><%= asterisk_device.asterisk_user %></td>
        <td><%= asterisk_device.admin_password %></td>
        <td><%= asterisk_device.mac_address %></td>
        <td><%= asterisk_device.account_code %></td>
        <td><%= asterisk_device.associated_user %></td>
        <td><%= link_to 'Show', asterisk_device %></td>
        <td><%= link_to 'Edit', edit_asterisk_device_path(asterisk_device) %></td>
        <td><%= link_to 'Destroy', asterisk_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asterisk device', new_asterisk_device_path %>
