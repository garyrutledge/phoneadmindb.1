<%= simple_form_for(@asterisk_user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :prefix %>
    <%= f.input :extension %>
    <%= f.input :did %>
    <%= f.input :display_name %>
    <%= f.input :password %>
    <%= f.input :voicemail_enabled %>
    <%= f.input :voicemail_password %>
    <%= f.input :email %>
    <%= f.input :faxenabled %>
    <%= f.association :asterisk_call_group %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
